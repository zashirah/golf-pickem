# DataGolf API key (required)
DATAGOLF_API_KEY=your_api_key_here

# Secret key for sessions (change in production)
SECRET_KEY=change-me-in-production

# Database URL (optional - for Supabase/PostgreSQL production deployment)
# If not set, will use local SQLite database
# Format: postgresql://user:password@host:port/database
# Get this from Supabase project settings
DATABASE_URL=
